{
    "id": "com.belmoussaoui.Obfuscate",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "44",
    "sdk" : "org.gnome.Sdk",
    "sdk-extensions" : [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command" : "obfuscate",
    "finish-args" : [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri"
    ],
    "build-options" : {
        "append-path": "/usr/lib/sdk/rust-stable/bin",
        "env": {
            "CARGO_HOME": "/run/build/obfuscate/cargo"
        }
    },
    "modules" : [
        {
            "name" : "obfuscate",
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "archive",
                    "url" : "https://gitlab.gnome.org/World/obfuscate/uploads/f42e783cb0df3551236ccdbd04a08928/obfuscate-0.0.8.tar.xz",
                    "sha256": "ac682e7edb39d3f21d4ba31192c19408a0c270818e0bbba4a0ed2a9aa4fe9f9b"
                }
            ]
        }
    ]
}

