{
    "app-id": "com.belmoussaoui.Obfuscate",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "3.36",
    "sdk" : "org.gnome.Sdk",
    "sdk-extensions" : [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command" : "obfuscate",
    "finish-args" : [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri"
    ],
    "build-options" : {
        "append-path": "/usr/lib/sdk/rust-stable/bin",
        "env": {
            "CARGO_HOME": "/run/build/obfuscate/cargo"
        }
    },
    "modules" : [
        {
            "name" : "obfuscate",
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "archive",
                    "url" : "https://gitlab.gnome.org/World/obfuscate/uploads/c0195c28c183eacb449702c140ddbc13/obfuscate-0.0.2.tar.xz",
                    "sha256": "cbabef4d24676887f545093ce718b8727926e45536e7ce74425161b147ec1ecb"
                }
            ]
        }
    ]
}

