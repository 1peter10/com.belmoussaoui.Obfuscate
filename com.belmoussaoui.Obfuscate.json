{
    "app-id": "com.belmoussaoui.Obfuscate",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "42",
    "sdk" : "org.gnome.Sdk",
    "sdk-extensions" : [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command" : "obfuscate",
    "finish-args" : [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri"
    ],
    "build-options" : {
        "append-path": "/usr/lib/sdk/rust-stable/bin",
        "env": {
            "CARGO_HOME": "/run/build/obfuscate/cargo"
        }
    },
    "modules" : [
        {
            "name" : "obfuscate",
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "archive",
                    "url" : "https://gitlab.gnome.org/World/obfuscate/uploads/96fbec361160ab152c05053badd4ed60/obfuscate-0.0.6.tar.xz",
                    "sha256": "9713e5d686e08237515b5d566509224120c9419141dcf2fd096aa9655e1d9ae8"
                }
            ]
        }
    ]
}

