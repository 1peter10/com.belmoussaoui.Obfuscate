{
    "app-id": "com.belmoussaoui.Obfuscate",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "42",
    "sdk" : "org.gnome.Sdk",
    "sdk-extensions" : [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command" : "obfuscate",
    "finish-args" : [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri"
    ],
    "build-options" : {
        "append-path": "/usr/lib/sdk/rust-stable/bin",
        "env": {
            "CARGO_HOME": "/run/build/obfuscate/cargo"
        }
    },
    "modules" : [
        {
            "name" : "obfuscate",
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "archive",
                    "url" : "https://gitlab.gnome.org/World/obfuscate/uploads/3c2fa3ef545fb036d8710eced062e8c0/obfuscate-0.0.5.tar.xz",
                    "sha256": "1a3a610d2258d637cac36cc7404f0a10c2ea13ab207b874fd3e293e7e8c1cf9b"
                }
            ]
        }
    ]
}

